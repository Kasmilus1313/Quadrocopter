CREATE TABLE Configurations (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  name TEXT NOT NULL UNIQUE,
  ip_address TEXT NOT NULL,
  password TEXT NOT NULL,
  data_type NUMERIC,
  actual BOOL
);

CREATE TABLE Sensors (
  id INTEGER PRIMARY KEY AUTOINCREMENT,
  name TEXT NOT NULL UNIQUE,
  kp INTEGER,
  ki INTEGER,
  kd INTEGER,
  tf INTEGER
);
